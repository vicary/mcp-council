{
  "name": "@vicary/mcp-council",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "unstable": ["kv"],
  "tasks": {
    "dev": "deno run --allow-all --watch src/main.ts",
    "compile": "deno compile --allow-all --output=mcp-council src/main.ts",
    "debug": "deno run --allow-all --unstable-kv src/tui.ts",
    "test": "deno test --allow-read --allow-write --allow-env --allow-net"
  },
  "imports": {
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.25.1",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/cli": "jsr:@std/cli@^1.0.24",
    "@std/fmt": "jsr:@std/fmt@^1.0.5",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "zod": "npm:zod@^4.2.1"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  }
}
