{
  "name": "@vicary/mcp-council",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "unstable": ["kv"],
  "tasks": {
    "dev": "deno run -A --watch src/main.ts",
    "compile": "deno compile -A --output=mcp-council src/main.ts",
    "debug": "deno run -A --unstable-kv src/tui.ts",
    "test": "deno test -RW"
  },
  "imports": {
    "@cliffy/ansi": "jsr:@cliffy/ansi@^1.0.0-rc.8",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.8",
    "less-pager-mini": "npm:less-pager-mini@^1.4.0",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.25.1",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "@std/ulid": "jsr:@std/ulid@^1.0.0",
    "zod": "npm:zod@^4.2.1"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  }
}
